{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "title": "The root schema",
  "description": "Array of classes and their execution counts",
  "default": [],
  "examples": [
    [
      {
        "className": "org.jabref.gui.DefaultInjector",
        "methods": [
          {
            "methodName": "injectMembers",
            "executionCounts": [
              {
                "lineNumber": 73,
                "count": 3
              }
            ]
          }
        ]
      },
      {
        "className": "org.jabref.gui.JabRefDialogService",
        "methods": [
          {
            "methodName": "notify",
            "executionCounts": [
              {
                "lineNumber": 274,
                "count": 5
              }
            ]
          }
        ]
      }
    ]
  ],
  "additionalItems": true,
  "items": {
    "anyOf": [
      {
        "$id": "#/items/anyOf/0",
        "type": "object",
        "title": "Per class execution count schema",
        "default": {},
        "examples": [
          {
            "className": "org.jabref.gui.DefaultInjector",
            "methods": [
              {
                "methodName": "injectMembers",
                "executionCounts": [
                  {
                    "lineNumber": 73,
                    "count": 3
                  }
                ]
              }
            ]
          }
        ],
        "required": [
          "className",
          "methods"
        ],
        "additionalProperties": true,
        "properties": {
          "className": {
            "$id": "#/items/anyOf/0/properties/className",
            "type": "string",
            "title": "The className schema",
            "description": "The fully qualified class name",
            "default": "",
            "examples": [
              "org.jabref.gui.DefaultInjector"
            ]
          },
          "methods": {
            "$id": "#/items/anyOf/0/properties/methods",
            "type": "array",
            "title": "The methods schema",
            "description": "Array of methods with execution counts",
            "default": [],
            "examples": [
              [
                {
                  "methodName": "injectMembers",
                  "executionCounts": [
                    {
                      "lineNumber": 73,
                      "count": 3
                    }
                  ]
                }
              ]
            ],
            "additionalItems": true,
            "items": {
              "anyOf": [
                {
                  "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0",
                  "type": "object",
                  "title": "Per method execution counts",
                  "default": {},
                  "examples": [
                    {
                      "methodName": "injectMembers",
                      "executionCounts": [
                        {
                          "lineNumber": 73,
                          "count": 3
                        }
                      ]
                    }
                  ],
                  "required": [
                    "methodName",
                    "executionCounts"
                  ],
                  "additionalProperties": true,
                  "properties": {
                    "methodName": {
                      "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/methodName",
                      "type": "string",
                      "title": "The methodName schema",
                      "description": "Name of the method, without parantheses or arguments",
                      "default": "",
                      "examples": [
                        "injectMembers"
                      ]
                    },
                    "executionCounts": {
                      "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/executionCounts",
                      "type": "array",
                      "title": "The executionCounts schema",
                      "description": "Execution counts per line in the method",
                      "default": [],
                      "examples": [
                        [
                          {
                            "lineNumber": 73,
                            "count": 3
                          }
                        ]
                      ],
                      "additionalItems": true,
                      "items": {
                        "anyOf": [
                          {
                            "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/executionCounts/items/anyOf/0",
                            "type": "object",
                            "title": "Execution counts per line",
                            "default": {},
                            "examples": [
                              {
                                "lineNumber": 73,
                                "count": 3
                              }
                            ],
                            "required": [
                              "lineNumber",
                              "count"
                            ],
                            "additionalProperties": true,
                            "properties": {
                              "lineNumber": {
                                "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/executionCounts/items/anyOf/0/properties/lineNumber",
                                "type": "integer",
                                "title": "The lineNumber schema",
                                "description": "Source code line number",
                                "default": 0,
                                "examples": [
                                  73
                                ]
                              },
                              "count": {
                                "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/executionCounts/items/anyOf/0/properties/count",
                                "type": "integer",
                                "title": "The count schema",
                                "description": "Execution count for the line",
                                "default": 0,
                                "examples": [
                                  3
                                ]
                              }
                            }
                          }
                        ],
                        "$id": "#/items/anyOf/0/properties/methods/items/anyOf/0/properties/executionCounts/items"
                      }
                    }
                  }
                }
              ],
              "$id": "#/items/anyOf/0/properties/methods/items"
            }
          }
        }
      }
    ],
    "$id": "#/items"
  }
}
